{
  "name": "drupal/dorion",
  "description": "Dorion is a base theme for Drupal. It has Bootstrap, Sass, BrowserSync and Font Awesome built-in.",
  "type": "drupal-theme",
  "homepage": "https://www.drupal.org/project/dorion",
  "authors": [
    {
      "name": "Paul Glazkow",
      "email": "glazzkow@gmail.com",
      "role": "Maintainer"
    }
  ],
  "support": {
    "issues": "https://www.drupal.org/project/issues/dorion"
  },
  "license": "GPL-2.0+",
  "minimum-stability": "dev",
  "require": {
    "drupal/components": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "Drush\\Commands\\dorion\\": "Commands/dorion/"
    }
  },
  "extra": {
    "drush": {
      "services": {
        "drush.services.yml": "^9"
      }
    }
  }
}
